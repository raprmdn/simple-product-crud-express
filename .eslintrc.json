{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "standard",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "indent": ["warn", 4],
        "semi": ["warn", "always"],
        "no-console": "off",
        "consistent-return": ["warn", { "treatUndefinedAsUnspecified": true }],
        "no-throw-literal": "off"
    }
}
